{
  "name": "andrew-bot-discord",
  "version": "1.0.0",
  "description": "AndrewBot but for the discord platform",
  "author": "Al Petersen <theoperatore@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "private": true,
  "scripts": {
    "dev": "ts-run server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "discord.js": "^12.0.2",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@kirbysayshi/ts-run": "^3.0.1",
    "@types/node": "^13.9.2",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  }
}
