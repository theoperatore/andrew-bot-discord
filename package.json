{
  "name": "andrew-bot-discord",
  "version": "0.0.15",
  "description": "AndrewBot but for the discord platform",
  "author": "Al Petersen <theoperatore@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "private": true,
  "scripts": {
    "dev": "GIT_SHA=$(git rev-parse --short HEAD) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) ts-run -r dotenv/config src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "cron": "^1.8.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@kirbysayshi/ts-run": "^5.0.1",
    "@types/cron": "^1.7.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/ws": "^7.2.6",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
